<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuang.autocode.mapper.SysAutoCode">
	
	<select id="selectSysAutoCodeById" resultType="com.zhuang.autocode.model.SysAutoCode">
		select * from
		sys_autocode where id = #{id}
	</select>

	<insert id="addSysAutoCode" parameterType="com.zhuang.autocode.model.SysAutoCode">
	
		INSERT INTO  sys_autocode(id,expression,description)
		VALUES(#{id},#{expression},#{description})
		
	</insert>
		
</mapper>

